<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  
  # Permitir acesso direto a sitemap.xml e robots.txt
  RewriteRule ^sitemap\.xml$ - [L]
  RewriteRule ^robots\.txt$ - [L]
  
  # N찾o redirecionar index.html
  RewriteRule ^index\.html$ - [L]
  
  # Redirecionar apenas se o arquivo n찾o existir e n찾o for um diret처rio
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>

